package org.jgs.oop.day01;

public class TestTeacher {

	public static void main(String[] args) {
		
		//创建教师对象
		Teacher teacher = new Teacher();
		
		//赋值
		teacher.name = "王老师";
		teacher.age = 30;
		teacher.course = "使用java语言理解程序逻辑";
		teacher.pro = "计算机";
		
		
		System.out.println(teacher.name);
		System.out.println("专业方向:"+teacher.pro);
		System.out.println("年龄:"+teacher.age);
		System.out.println("授课内容:"+teacher.course);

	}

}
