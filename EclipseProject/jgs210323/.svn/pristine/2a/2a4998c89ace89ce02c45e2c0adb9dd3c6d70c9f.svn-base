package org.jgs.day04;
/*
 * 分别计算1到100的偶数和奇数和
 */
public class Test08 {
	public static void main(String[] args) {
		
		int sum1 = 0;
		int sum2 = 0;
		for (int i = 1; i <101; i++) {
			
			if (i%2==0) {
				sum2+=i;
			}else {
				sum1+=i;
			}
			
		}
		//打印输出
		System.out.println("偶数和："+sum2);
		System.out.println("奇数和："+sum1);
		
		
	}
}
