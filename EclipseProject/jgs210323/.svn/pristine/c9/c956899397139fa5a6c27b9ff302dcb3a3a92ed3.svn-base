package org.jgs.day02;
/**
 * 赋值运算符
 * =  赋值
 * += -= *= /=  %=
 * 
 * @author mosin
 * @DATE 2021年3月29日
 */
public class Operator05 {

	public static void main(String[] args) {
		
		int b = 15;
		int a = 10;
		a+=2;  //a = a+2
		System.out.println(a);
		a-=3;  //a = a-3
		System.out.println(a);
		
		a*=2;  //a = a*2
		System.out.println(a);
		
		a/=2;
		System.out.println(a);  //9
		a%=3; //a = a%3
		System.out.println(a);
	
		//三目运算    条件表达式  ？  表达式1:表达式2
		
		int c = a>b ? a:b;
		System.out.println(c);
		
		
	}

}
