package org.jgs.day06;
/*
 * 寻找指定元素是否在数组中，如果在输出其下标 不在则提示不存在
 */
public class ArrayDemo05 {

	public static void main(String[] args) {
		
		int[]  num = {10,2,30,13,20,15};
		int temp= 2;
		int i ;
		boolean flag = false;
		for ( i = 0; i < num.length; i++) {
			if (num[i]==temp) {
				flag = true;
				break;
			}
		}
		
		if (flag) {
			System.out.println("该数字的下标是："+i);
		}else {
			System.out.println("该数字不存在");
		}
	}

}
