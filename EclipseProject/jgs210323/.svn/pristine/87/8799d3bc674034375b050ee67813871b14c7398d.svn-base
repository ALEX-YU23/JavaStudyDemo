package org.jgs.day04;
/*
 
 打印如下的图形
 *
 **
 ***
 ****
 */
public class Test01 {

	public static void main(String[] args) {
		
		//外层循环控制打印的行数
		for (int i = 1; i < 5; i++) {
			//内层循环控制每行打印次数
			for (int j = 1; j <=i; j++) {
				System.out.print("*");
			}
			//一行打印完毕 换行
			System.out.println();
		}
		

	}

}
