package org.jgs.day02;

import java.lang.invoke.VarHandle;
import java.util.Scanner;

/**
 * 关系运算符  返回值是布尔类型
 * == 相等
 * ！=  不等
 * >  大于
 * <小于
 * >=
 * <=
 * @author mosin
 * @DATE 2021年3月29日
 */
public class Operator02 {

	public static void main(String[] args) {
		
		int a = 10;
		int b = 12;
		
		//==
		 boolean c =  a==b;
		 System.out.println(c);
		 System.out.println(a==b);
		 System.out.println(a>b);
		 System.out.println(a<b);
		 System.out.println(a>=b);
		 System.out.println(a<=b);
		 System.out.println(a!=b);

	}

}
