package org.jgs.Thread.day01;
/**
 * 使用lambda表达式创建线程
 * 
 * @author mosin
 * @DATE 2021年5月27日
 */
public class ThreadDemo4 {
	
	public static void main(String[] args) {
		
		new Thread(()->{
			for (int i = 0; i < 100; i++) {
				System.out.println("瞅你咋地！");
			}
		}).start();
		
		new Thread(()->{
			for (int i = 0; i < 100; i++) {
				System.out.println("你瞅啥！");
			}
		}).start();

	}

}


